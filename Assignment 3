 
#1
dict={'bats' : 50,"balls":100,"stumps":100}
sum=0
for x in dict:
    sum=sum+dict[x]
print(sum)

#2
d={'x':10,'y':20,'z':30}
a=d["x"]
b=d['y']
c=d['z']
print(a,b,c)

#3
dict_1={"x":1,"y":2,"z":3}
dict_2={"a":4,"b":5,"c":6}
dict_3={}
for d in (dict_1,dict_2):dict_3.update(d)
print(dict_3)

#4.
di={"x":10,"y":20}
di["x"]=31
print(di)

#5
